<?xml version="1.0" encoding="UTF-8"?>
<!-- Component that contains a subcomponent producing an exception during init. -->
<componenttype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex http://jadex.sourceforge.net/jadex-component-2.5.xsd"
	name="BrokenInit" package="jadex.micro.testcases">
	
	<componenttypes>
		<componenttype name="BrokenInitAgent" filename="BrokenInitAgent.class"/>
	</componenttypes>
	
	<configurations>
		<configuration name="default">
			<components>
				<component type="BrokenInitAgent"/>
			</components>
		</configuration>
	</configurations>
	
</componenttype>
