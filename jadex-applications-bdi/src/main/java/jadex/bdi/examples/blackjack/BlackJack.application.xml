<!--
	<H3>BlackJack application descriptor</H3>
-->
<applicationtype xmlns="http://jadex.sourceforge.net/jadex-application"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex-application 
	                    http://jadex.sourceforge.net/jadex-application-2.0.xsd"
	name="BlackJack" package="jadex.bdi.examples.blackjack">
	
	<componenttypes>
		<componenttype name="Manager" filename="jadex/bdi/examples/blackjack/manager/Manager.agent.xml"/>
		<componenttype name="Dealer" filename="jadex/bdi/examples/blackjack/dealer/Dealer.agent.xml"/>
		<componenttype name="Player" filename="jadex/bdi/examples/blackjack/player/Player.agent.xml"/>
	</componenttypes>
	
	<applications>
		<application name="Only Manager">
			<components>
				<component type="Manager" master="true"/>
			</components>
		</application>
	</applications>

</applicationtype>
