<?xml version="1.0" ?><bpmn:definitions targetNamespace="http://www.activecomponents.org/bpmn/" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:jadex="http://www.activecomponents.org/bpmnextensions" xmlns:jadexvisual="http://www.activecomponents.org/bpmnvisualextensions" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <bpmn:extension>
    <jadex:modelname>MessageReceival</jadex:modelname>
    <jadex:description>&lt;h3&gt;Message receival process&lt;/h3&gt;The Message Receival process waits for a predefined number of messages and then terminates.</jadex:description>
    <jadex:package>jadex.bpmn.examples.message</jadex:package>
    <jadex:imports>
      <jadex:import>jadex.bpmn.examples.execute</jadex:import>
      <jadex:import>jadex.commons.*</jadex:import>
    </jadex:imports>
    <jadex:arguments>
      <jadex:argument name="message_number" type="int">
        <jadex:description>The number of messages to receive.</jadex:description>
        <jadex:value>10</jadex:value>
      </jadex:argument>
    </jadex:arguments>
  </bpmn:extension>
  <bpmn:process id="_Q5erIc-aEd6NWbZHdUI2WQ" name="Pool">
    <bpmn:task id="_Q5erI8-aEd6NWbZHdUI2WQ" name="PreMessage">
      <bpmn:incoming>_6WZS8dBNEd6NWbZHdUI2WQ</bpmn:incoming>
      <bpmn:incoming>_b0-_QdByEd6NWbZHdUI2WQ</bpmn:incoming>
      <bpmn:outgoing>_bjpHwc-aEd6NWbZHdUI2WQ</bpmn:outgoing>
      <bpmn:extensionElements>
        <jadex:parameter direction="inout" name="i" type="int">0</jadex:parameter>
      </bpmn:extensionElements>
    </bpmn:task>
    <bpmn:startEvent id="_5_vtgdBNEd6NWbZHdUI2WQ">
      <bpmn:outgoing>_6WZS8dBNEd6NWbZHdUI2WQ</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:intermediateCatchEvent id="_bjf90c-aEd6NWbZHdUI2WQ" name="Wait for Message">
      <bpmn:incoming>_bjpHwc-aEd6NWbZHdUI2WQ</bpmn:incoming>
      <bpmn:outgoing>_ZD05kc-eEd6NWbZHdUI2WQ</bpmn:outgoing>
      <bpmn:messageEventDefinition/>
      <bpmn:extensionElements>
        <jadex:property name="filter">IFilter.ALWAYS</jadex:property>
      </bpmn:extensionElements>
    </bpmn:intermediateCatchEvent>
    <bpmn:task id="_ZDrIkc-eEd6NWbZHdUI2WQ" name="MessageReceived">
      <bpmn:incoming>_ZD05kc-eEd6NWbZHdUI2WQ</bpmn:incoming>
      <bpmn:outgoing>_whyoYRY1Ed-yC473XtaJZQ</bpmn:outgoing>
      <bpmn:extensionElements>
        <jadex:parameter direction="inout" name="i" type="int">i</jadex:parameter>
      </bpmn:extensionElements>
    </bpmn:task>
    <bpmn:exclusiveGateway default="_SMJwkRY-Ed-yC473XtaJZQ" id="_vk2fARY1Ed-yC473XtaJZQ">
      <bpmn:incoming>_whyoYRY1Ed-yC473XtaJZQ</bpmn:incoming>
      <bpmn:outgoing>_b0-_QdByEd6NWbZHdUI2WQ</bpmn:outgoing>
      <bpmn:outgoing>_SMJwkRY-Ed-yC473XtaJZQ</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="_SL21oRY-Ed-yC473XtaJZQ" name="Print results">
      <bpmn:incoming>_SMJwkRY-Ed-yC473XtaJZQ</bpmn:incoming>
      <bpmn:outgoing>_yNhxcRY1Ed-yC473XtaJZQ</bpmn:outgoing>
      <bpmn:extensionElements>
        <jadex:class>jadex.bpmn.runtime.task.PrintTask</jadex:class>
        <jadex:parameter direction="in" name="text" type="String">$component.getId().getLocalName()+&quot; received &quot;+message_number+&quot; messages.&quot;</jadex:parameter>
      </bpmn:extensionElements>
    </bpmn:task>
    <bpmn:endEvent id="_yNYAcRY1Ed-yC473XtaJZQ">
      <bpmn:incoming>_yNhxcRY1Ed-yC473XtaJZQ</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="_bjpHwc-aEd6NWbZHdUI2WQ" sourceRef="_Q5erI8-aEd6NWbZHdUI2WQ" targetRef="_bjf90c-aEd6NWbZHdUI2WQ">
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="_6WZS8dBNEd6NWbZHdUI2WQ" sourceRef="_5_vtgdBNEd6NWbZHdUI2WQ" targetRef="_Q5erI8-aEd6NWbZHdUI2WQ">
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="_ZD05kc-eEd6NWbZHdUI2WQ" sourceRef="_bjf90c-aEd6NWbZHdUI2WQ" targetRef="_ZDrIkc-eEd6NWbZHdUI2WQ">
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="_whyoYRY1Ed-yC473XtaJZQ" sourceRef="_ZDrIkc-eEd6NWbZHdUI2WQ" targetRef="_vk2fARY1Ed-yC473XtaJZQ">
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="_b0-_QdByEd6NWbZHdUI2WQ" sourceRef="_vk2fARY1Ed-yC473XtaJZQ" targetRef="_Q5erI8-aEd6NWbZHdUI2WQ">
      <bpmn:conditionExpression>i+1 &lt; message_number</bpmn:conditionExpression>
      <bpmn:extensionElements>
        <jadex:parametermapping name="i">i+1</jadex:parametermapping>
      </bpmn:extensionElements>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="_SMJwkRY-Ed-yC473XtaJZQ" sourceRef="_vk2fARY1Ed-yC473XtaJZQ" targetRef="_SL21oRY-Ed-yC473XtaJZQ">
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="_yNhxcRY1Ed-yC473XtaJZQ" sourceRef="_SL21oRY-Ed-yC473XtaJZQ" targetRef="_yNYAcRY1Ed-yC473XtaJZQ">
    </bpmn:sequenceFlow>
    <bpmn:extensionElements>
    </bpmn:extensionElements>
  </bpmn:process>
