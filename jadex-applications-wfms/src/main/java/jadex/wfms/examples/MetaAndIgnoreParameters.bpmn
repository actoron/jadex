    <vertices xmi:type="bpmn:Activity" xmi:id="_u_NmsxroEd-WKMiNMlV1zA" iD="_u_NmshroEd-WKMiNMlV1zA" outgoingEdges="_bJdK8RrtEd-WKMiNMlV1zA" incomingEdges="_zLFxQRroEd-WKMiNMlV1zA" name="Categories_Demo" lanes="_xJ-OERroEd-WKMiNMlV1zA" activityType="Task">
      <eAnnotations xmi:type="ecore:EAnnotation" xmi:id="_v9aUUBroEd-WKMiNMlV1zA" source="jadex">
        <details xmi:type="ecore:EStringToStringMapEntry" xmi:id="_v9a7YBroEd-WKMiNMlV1zA" key="class" value="jadex.wfms.client.task.WorkitemTask"/>
        <details xmi:type="ecore:EStringToStringMapEntry" xmi:id="__bhBEBroEd-WKMiNMlV1zA" key="parameters" value="in␋Information␋Text␋new Text(&quot;&lt;html>&lt;center>&lt;h2>Categories Demo&lt;/h2>&lt;p>This workitem demonstrates how to group parameters into categories using the Meta-Parameter META_category:[category_name].&lt;/p>&lt;/center>&lt;/html>&quot;)␟in␋Foo_Cat_Information␋Text␋new Text(&quot;&lt;html>&lt;center>&lt;h3>This is the first category called \&quot;Foo\&quot;&lt;/h3>&lt;/center>&lt;/html>&quot;)␟out␋Option_in_the_Foo_category␋boolean␋␟in␋Bar_Cat_Information␋Text␋new Text(&quot;&lt;html>&lt;center>&lt;h3>This is the second category called \&quot;Bar\&quot;&lt;/h3>&lt;/center>&lt;/html>&quot;)␟out␋String_Field_in_the_Bar_category␋String␋␟in␋META_category:Foo␋String[]␋new String[] {&quot;Foo_Cat_Information&quot;, &quot;Option_in_the_Foo_category&quot;}␟in␋META_category:Bar␋String[]␋new String[] {&quot;Bar_Cat_Information&quot;, &quot;String_Field_in_the_Bar_category&quot;}"/>
      </eAnnotations>
